<Window x:Class="nio2so.TSOTCP.Debug.Frontend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:nio2so.TSOTCP.Debug.Frontend"
        mc:Ignorable="d"
        Title="nio2so Debug Frontend" MinHeight="100" MinWidth="200" SizeToContent="WidthAndHeight" ResizeMode="NoResize">
    <StackPanel Margin="10">
        <TextBlock HorizontalAlignment="Center">nio2so - TCPQuaZar</TextBlock>
        <Separator Margin="0,10"/>
        <Button Name="ButtonPacketLog" Padding="10" Margin="0,0,0,10" Click="MainButton_Click">Show Packet Log</Button>
        <Button Name="ButtonClose" Padding="10" Click="MainButton_Click">Exit</Button>
    </StackPanel>
</Window>
